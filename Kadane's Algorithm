class Solution {
    int maxSubarraySum(int[] arr) {
        // Code here
        int currSum=arr[0];
        int maxSum=arr[0];
        
        int n=arr.length;
        for(int i=1; i<n; i++){
            currSum=Math.max(arr[i],currSum+arr[i]);
            maxSum=Math.max(maxSum,currSum);
        }
        return maxSum;
    }
}
